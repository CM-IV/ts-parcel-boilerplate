{
  "name": "ts-parcel-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate for ts and parcel bundler, based on Brad Traversy's JS boilerplate",
  "main": "index.ts",
  "scripts": {
    "dev": "npm run clean && parcel public/index.html --out-dir development - p 4000",
    "build": "parcel build public/*.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf -rf ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CM-IV/ts-parcel-boilerplate.git"
  },
  "keywords": [
    "parcel"
  ],
  "author": "CMIV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CM-IV/ts-parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/CM-IV/ts-parcel-boilerplate#readme",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime-corejs3": "^7.13.10",
    "bulma": "^0.9.2",
    "parcel-bundler": "^1.12.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
